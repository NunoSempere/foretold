{
	"name": "@foretold/client",
	"version": "0.2.0",
	"author": "Ozzie Gooen <ozzieagooen@gmail.com>",
	"keywords": [
		"BuckleScript"
	],
	"engines": {
		"node": ">=12.6.0",
		"yarn": ">=1.17.3"
	},
	"license": "MIT",
	"scripts": {
		"build-bsb": "bsb -make-world",
		"clean": "bsb -clean-world",
		"bsb-watch": "bsb -make-world -w",
		"parcel": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-guesstimate.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-gn1bwgJfK5Y6jfL6x7t6fB43ZAN3eSnT} parcel ./src/index.html --public-url /",
		"parcel-staging": "parcel ./src/index.html --public-url /",
		"parcel-production": "parcel ./src/index.html --public-url /",
		"prepare-dist": "mkdir -p dist && cp _redirects dist/_redirects",
		"replace-dist": "rm -rf .cache && rm -rf dist && yarn prepare-dist",
		"build": "parcel build ./src/index.html --no-cache --no-minify",
		"build-for-server": "API_URL=https://api.foretold.io/graphql AUTH0_DOMAIN=foretold.auth0.com AUTH0_CLIENT_ID=oIh53ohBnA4xqhaO8vZXa7PiW6TCXjMV yarn run build",
		"build-for-staging": "API_URL=https://prediction-backend-staging.herokuapp.com/graphql AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-oIh53ohBnA4xqhaO8vZXa7PiW6TCXjMV} yarn run build",
		"build-for-development": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-guesstimate.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-gn1bwgJfK5Y6jfL6x7t6fB43ZAN3eSnT} yarn run build",
		"server-deploy": "netlify deploy --dir=dist --prod",
		"build-and-deploy": "yarn run clean && yarn run build-bsb && yarn run build-for-server && yarn run server-deploy",
		"serve": "static-server ./dist -p ${PORT} -d -n ./dist/index.html",
		"serve-ws": "ws -d dist --spa index.html",
		"build-t": "npm run clean && npm run build-bsb && npm run build",
		"test": "jest --ci --bail --no-cache",
		"update-gql": "API_URL=${API_URL:-http://localhost:4000/graphql} yarn send-introspection-query ${API_URL}",
		"webpack": "webpack -w",
		"webpack:production": "NODE_ENV=production webpack",
		"find-interface-for-file": "bsc -bs-re-out",
		"analyze": "source-map-explorer dist/Index.adc0ecca.js"
	},
	"dependencies": {
		"@babel/preset-react": "7.0.0",
		"@foretold/cdf": "1.0.7",
		"@foretold/guesstimator": "1.0.0",
		"@glennsl/bs-jest": "0.4.8",
		"@glennsl/bs-json": "4.0.0",
		"@ncthbrt/re-secure-random-string": "2.0.0",
		"antd": "3.17.0",
		"apollo-cache-inmemory": "1.6.2",
		"apollo-client": "2.6.3",
		"apollo-link": "1.2.12",
		"apollo-link-context": "1.0.18",
		"apollo-link-error": "1.1.11",
		"apollo-link-http": "1.5.15",
		"apollo-link-ws": "1.0.18",
		"apollo-utilities": "1.3.2",
		"auth0-js": "9.11.2",
		"bs-ant-design": "git+https://git@github.com/Thinkei/bs-ant-design.git",
		"bs-css": "9.0.1",
		"bs-react-markdown": "0.0.9",
		"bs-moment": "0.4.4",
		"bs-platform": "5.0.6",
		"bs-reform": "6.0.0-gadt.5",
		"bsb-js": "1.1.7",
		"css-loader": "3.1.0",
		"d3": "5.9.7",
		"discrete-sampling": "1.0.3",
		"file-loader": "4.1.0",
		"financejs": "4.1.0",
		"graphql": "14.4.2",
		"graphql-tag": "2.10.1",
		"graphql_ppx": "0.2.8",
		"jstat": "1.9.0",
		"jwt-decode": "2.2.0",
		"less": "3.9.0",
		"less-loader": "5.0.0",
		"lib": "4.0.1",
		"lodash": "4.17.15",
		"mathjs": "5.10.3",
		"node-uuid": "1.4.8",
		"parcel-bundler": "1.12.3",
		"rationale": "0.1.10",
		"react": "16.8.6",
		"react-apollo": "2.5.8",
		"react-dom": "16.8.6",
		"react-markdown": "4.1.0",
		"react-icons-kit": "1.3.1",
		"reason-apollo": "0.15.2",
		"reason-react": "0.7.0",
		"source-map-explorer": "2.0.1",
		"static-server": "2.2.1",
		"style-loader": "0.23.1",
		"subscriptions-transport-ws": "0.9.16",
		"warning": "4.0.3",
		"babel-core": "6.26.3",
		"babel-plugin-import": "1.12.0",
		"babel-preset-es2015": "6.24.1",
		"babel-preset-stage-0": "6.24.1",
		"babel-runtime": "6.26.0",
		"parcel-plugin-less-js-enabled": "1.0.2",
		"webpack": "4.36.1",
		"webpack-cli": "3.3.6",
		"webpack-dev-server": "3.7.2"
	},
	"devDependencies": {
		"babel-preset-react": "^6.24.1",
		"parcel-plugin-html-externals": "^0.1.2"
	},
	"externals": {
		"/env.js": false
	}
}
