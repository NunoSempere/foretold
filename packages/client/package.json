{
  "name": "@foretold/client",
  "version": "0.2.0",
  "author": "Ozzie Gooen <ozzieagooen@gmail.com>",
  "keywords": [
    "BuckleScript"
  ],
  "engines": {
    "node": "12.6.0",
    "yarn": ">=1.16.0"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "build-bsb": "bsb -make-world",
    "clean": "bsb -clean-world",
    "bsb-watch": "bsb -make-world -w",
    "parcel": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-guesstimate.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-gn1bwgJfK5Y6jfL6x7t6fB43ZAN3eSnT} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} parcel ./src/index.html --public-url / --no-autoinstall -- watch",
    "parcel-staging": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-guesstimate.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-gn1bwgJfK5Y6jfL6x7t6fB43ZAN3eSnT} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} parcel build ./src/index.html --no-source-maps --no-autoinstall",
    "parcel-production": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-guesstimate.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-gn1bwgJfK5Y6jfL6x7t6fB43ZAN3eSnT} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} parcel build ./src/index.html --no-source-maps --no-autoinstall",
    "prepare-dist": "mkdir -p dist && cp _redirects dist/_redirects",
    "replace-dist": "rm -rf .cache && rm -rf dist && yarn prepare-dist",
    "build": "yarn parcel-production",
    "build-for-server": "API_URL=https://api.foretold.io/graphql AUTH0_DOMAIN=foretold.auth0.com AUTH0_CLIENT_ID=oIh53ohBnA4xqhaO8vZXa7PiW6TCXjMV INTERCOM_APP_ID=gx67sx4f NODE_ENV=development yarn parcel-production",
    "build-for-staging": "API_URL=https://prediction-backend-staging.herokuapp.com/graphql AUTH0_DOMAIN=${AUTH0_DOMAIN:-foretold.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-oIh53ohBnA4xqhaO8vZXa7PiW6TCXjMV} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} yarn parcel-staging",
    "build-for-development": "API_URL=${API_URL:-http://localhost:4000/graphql} AUTH0_DOMAIN=${AUTH0_DOMAIN:-guesstimate.auth0.com} AUTH0_CLIENT_ID=${AUTH0_CLIENT_ID:-gn1bwgJfK5Y6jfL6x7t6fB43ZAN3eSnT} INTERCOM_APP_ID=${INTERCOM_APP_ID:-gx67sx4f} yarn parcel",
    "build-and-deploy": "yarn clean && yarn build-bsb && yarn build-for-server && yarn server-deploy",
    "serve": "static-server ./dist -p ${PORT} -d -n ./dist/index.html",
    "serve-ws": "ws -d dist --spa index.html",
    "build-t": "yarn build-bsb && yarn build",
    "test": "jest --ci --bail --no-cache",
    "update-gql": "API_URL=${API_URL:-http://localhost:4000/graphql} yarn send-introspection-query ${API_URL}",
    "find-interface-for-file": "bsc -bs-re-out",
    "analyze": "source-map-explorer dist/Index.adc0ecca.js"
  },
  "dependencies": {
    "@foretold/cdf": "1.0.14",
    "@foretold/guesstimator": "1.0.10",
    "@foretold/components": "0.0.2",
    "@glennsl/bs-jest": "0.4.8",
    "@glennsl/bs-json": "3.0.0",
    "antd": "3.17.0",
    "auth0-js": "9.11.2",
    "bs-ant-design-alt": "2.0.0-alpha.28",
    "bs-css": "9.0.1",
    "bs-moment": "0.4.4",
    "bs-platform": "5.0.6",
    "bs-ken": "0.0.6",
    "gentype": "1.6.1",
    "bsb-js": "1.1.7",
    "d3": "5.9.7",
    "discrete-sampling": "1.0.3",
    "financejs": "4.1.0",
    "graphql": "14.4.2",
    "graphql-tag": "2.10.1",
    "graphql_ppx": "0.2.8",
    "jstat": "1.9.0",
    "jwt-decode": "2.2.0",
    "less": "3.9.0",
    "lib": "4.0.1",
    "lodash": "4.17.15",
    "mathjs": "5.10.3",
    "node-uuid": "1.4.8",
    "parcel-bundler": "1.12.3",
    "rationale": "0.1.10",
    "react-markdown": "4.1.0",
    "react-icons-kit": "1.3.1",
    "source-map-explorer": "2.0.1",
    "static-server": "2.2.1",
    "subscriptions-transport-ws": "0.9.16",
    "warning": "4.0.3",
    "parcel-plugin-less-js-enabled": "1.0.2",
    "apollo-client": "2.4.7",
    "react-apollo": "2.5.8",
    "bs-reform": "6.0.0-gadt.5",
    "reason-apollo": "0.16.4",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "reason-react": "git+https://github.com/RomanGalochkin/reason-react.git#0.7.0.beta.0",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "parcel-plugin-html-externals": "0.1.2"
  },
  "externals": {
    "/env.js": false
  }
}