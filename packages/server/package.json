{
  "name": "@foretold/server",
  "version": "1.0.0",
  "description": "Foretold Server",
  "author": "Ozzie Gooen <ozzieagooen@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "engines": {
    "node": ">=12.6.0",
    "yarn": ">=1.17.3"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config\"",
    "dev:production": "DATABASE_URL=postgres://ozziegooen@127.0.0.1:5432/predictions NODE_ENV=production yarn run dev",
    "thing": "nodemon src/bots/primary.js --exec \"node -r dotenv/config\"",
    "resolutions": "nodemon src/bots/resolution.js --exec \"node -r dotenv/config\"",
    "inspect": "nodemon --inspect src/index.js",
    "test": "jest --watch",
    "test:cv": "jest --coverage --forceExit",
    "test:ci": "jest --forceExit",
    "lint": "esw -w src test",
    "serve": "node ./src/index.js",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "yarn run db:drop; yarn run db:create; yarn run db:migrate"
  },
  "keywords": [
    "foretold",
    "predictions",
    "cdf"
  ],
  "dependencies": {
    "@slack/client": "^5.0.2",
    "apollo-server": "^2.7.2",
    "apollo-server-express": "^2.7.2",
    "auth0": "^2.18.0",
    "body-parser": "^1.19.0",
    "body-parser-graphql": "^1.1.0",
    "change-case": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "graphql-client": "^2.0.1",
    "graphql-middleware": "^3.0.3",
    "graphql-relay": "^0.6.0",
    "graphql-sequelize": "^9.3.6",
    "graphql-shield": "^6.0.4",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mustache": "^3.0.1",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.0",
    "pg": "^7.12.0",
    "sequelize": "^5.11.0",
    "github-api": "^3.2.2",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@types/lodash": "^4.14.136",
    "@types/express": "^4.17.0",
    "@types/nodemailer": "^6.2.1",
    "@types/moment": "^2.13.0",
    "@types/sequelize": "^4.28.4",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "eslint-watch": "^6.0.0",
    "graphql_ppx": "^0.2.8",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "clearMocks": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/manuals/"
    ],
    "setupFiles": [
      "./src/jest-globals.js"
    ]
  }
}
